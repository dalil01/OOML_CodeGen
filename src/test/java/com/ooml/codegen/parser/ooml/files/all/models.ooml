// Models

~ models {

	~ interfaces {

		+ interface IUserInfo:
			getUSerInfo() // public by default
			+ displayInfo() // Return void by default

	}

	~ enums {
		+ enum Role:
		ADMIN = "dd", CUSTOMER;


	}

	+ abstract class User -> IUSerInfo {
		- id: int;
		#: firstname, lastname: string
	       role: Role
		+ User()
		+ User(firstname, lastname: string)
	}

	+ class Admin -> User:

	+ class Customer -> User:

}